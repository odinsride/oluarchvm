! Colors

! special
*.foreground:   #ffffff
*.background:   #30313a
*.cursorColor:  #ffffff

! black
*.color0:       #3d3d3d
*.color8:       #4d4d4d

! red
*.color1:       #6673bf
*.color9:       #899aff

! green
*.color2:       #3ea290
*.color10:      #52ad91

! yellow
*.color3:       #b0ead9
*.color11:      #98c9bb

! blue
*.color4:       #477ab3
*.color12:      #477ab3

! magenta
*.color5:       #596196
*.color13:      #7882bf

! cyan
*.color6:       #8292b2
*.color14:      #95a7cc

! white
*.color7:       #c8cacc
*.color15:      #edeff2

! "Fake" transparency
!URxvt*transparent: true
!URxvt*shading: 20 

! Fonts
URxvt.font: 		xft:Roboto Mono for Powerline:style=Regular:pixelsize=15:antialias=true:hinting=true
URxvt.boldFont: 	xft:Roboto Mono for Powerline:bold:pixelsize=15:antialias=true:hinting=true
URxvt.italicFont:	xft:Roboto Mono for Powerline:italic:pixelsize=15:antialias=true:hinting=true
URxvt.boldItalicFont:	xft:Roboto Mono for Powerline:bold:italic:pixelsize=15:antialias=true:hinting=true
URxvt.letterSpace: 0 


!UI
URxvt*borderless: true
URxvt.internalBorder: 20
URxvt*scrollBar: false
URxvt*saveLines: 20000
URxvt*dynamicColors: true
URxvt*termName: rxvt-unicode
URxvt*fading: 10

! Transparency
URxvt.depth: 32
URxvt.background: [85]#1c2023
URxvt.intensityStyles: false
!URxvt*transparent: true
!URxvt*shading: 10
!URxvt*tintColor: #000000
!URxvt*blurRadius: 5

!Keys
URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard
URxvt.iso14755: true
URxvt.iso14755_52: false
